          "returns_comment": false,
          "parameters": [
            {
              "name": "_bind",
              "comment": "a boolean value to bind mouse events if set to `true`. If `false`, all mouse events will be unbound. Default is true.",
              "type": "boolean",
              "default": "true"
            }
